{
    "name": "code-foundation/flow-config",
    "description": "Cascading key/value configuration of preferences for platforms, installs, and users.",
    "type": "library",
    "keywords": ["config", "configuration", "users", "entities"],
    "homepage": "https://github.com/code-foundation/flow-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Edward Murrell",
            "email": "edward.murrell@codefoundation.com.au",
            "homepage": "https://codefoundation.com.au"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "^0.12.5"
    },
    "autoload": {
        "psr-4": {
            "CodeFoundation\\FlowConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeFoundation\\FlowConfig\\Tests\\": "tests/"
        }
